{
    "name": "fas/exportable",
    "type": "library",
    "description": "Fast and simple var exporter",
    "keywords": [
        "var_export",
        "closure"
    ],
    "homepage": "https://github.com/gielfeldt/fas-exportable",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Gielfeldt",
            "email": "thomas@gielfeldt.dk"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "opis/closure": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Fas\\Exportable\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpmd/phpmd": "^2.10",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Fas\\Exportable\\Tests\\": "tests/"
        }
    }
}
